{
    "name": "zsfoto/valicake",
    "description": "NOT FINISH! ValiCake Admin plugin for CakePHP 5.x",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "ValiCake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ValiCake\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
